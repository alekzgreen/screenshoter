{
	"name": "Frame - Chrome Screenshots",
	"short_name": "Frame",
	"version": "1.0.16",
	"description": "Frame",
	"homepage_url": "https://chrome.google.com/webstore/detail/jlbnmcnmjhcgdnbkdlgmibahbffjlnpa",
	"manifest_version": 2,
	"options_ui": {
	    "page": "settings.html",
	    "open_in_tab": true
  	},
	"permissions": [
		"tabs", 
		"webNavigation", 
		"webRequest", 
		"storage",
		"unlimitedStorage",
		"activeTab",
		"notifications", 
		"desktopCapture",
		"contextMenus",
		"debugger",
		"downloads"
	],
	"background": {
		"scripts": ["vendors/bundle.js", "bg/bundle.js"],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "icon-disabled-32.png",
		"default_title": "Frame - Chrome Screenshots"
	},
	"icons": {
		"32": "icon-32.png",
		"64": "icon-64.png",
		"128": "icon-128.png"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": ["content/*.svg"]
}